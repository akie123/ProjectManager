<h1 style="text-align:center;padding-top:1rem">Project Details!</h1>
<br>
<br>
<div class="container-fluid">


    <h2>Yours Projects</h2>

  <div class="tab">
    <table class="table">
        <thead>
        <tr>

            <th scope="col">#</th>
            <th scope="col">Project Name</th>
            <th scope="col">Roll-No</th>
            <th scope="col">Semester</th>
            <th scope="col">Project Details</th>
            <th scope="col">Technology Used</th>

        </tr>
        </thead>
        <tbody>
          <% val.forEach(function(to){  %>
                  <tr>
                    <td><%= to.S20200010001id %></td>
                    <td><%= to.S20200010001ProjectName %></td>
                    <td><%= to.S20200010001RollNo %></td>
                    <td><%= to.S20200010001Semester %></td>
                    <td><%= to.S20200010001ProjectDetails %></td>
                    <td><%= to.S20200010001TechUsed %></td>
                  </tr>
                <%})%>

        </tbody>
    </table>
  </div>
  <h2>Add Project</h2>

  <div class="tab1">

    <form action="/" method="post" >
      <div class="form-group">
        <label for="projname">Project Name</label>
        <input type="text" class="form-control" id="projname"  placeholder="Enter Project Name">

      </div>
        <div class="form-group">
            <label for="roll">Roll-No</label>
            <input type="text" class="form-control" id="roll"  placeholder="Enter Roll Number">

        </div>
        <div class="form-group">
            <label for="sem">Semester</label>
            <input type="text" class="form-control" id="sem"  placeholder="Enter Current Semester">

        </div>

        <div class="form-group">
            <label for="projdet">Project Details</label>
            <textarea class="form-control" id="projdet"  placeholder="Enter Project Details" rows="3"></textarea>


        </div>
        <div class="form-group">
            <label for="tech">Technology Used</label>
            <input type="text" class="form-control" id="tech"  placeholder="Enter Technology Used">

        </div>
        <div class="butt">
          <button type="submit" id="but" class="btn btn-dark">Add Project</button>
        </div>

    </form>


  </div>

